<?xml version="1.0" encoding="UTF-8"?>
<session xmlns="http://www.appserver.io/appserver">
    <session-type>Stateful</session-type>
    <epb-name>UserProcessor</epb-name>
    <epb-class>AppserverIo\Apps\Example\Services\UserProcessor</epb-class>
    <pre-attach>
        <lifecycle-callback-method>newAttach</lifecycle-callback-method>
    </pre-attach>
    <post-detach>
        <lifecycle-callback-method>newDetach</lifecycle-callback-method>
    </post-detach>
    <pre-passivate>
        <lifecycle-callback-method>newPassivate</lifecycle-callback-method>
    </pre-passivate>
    <post-activate>
        <lifecycle-callback-method>newActivate</lifecycle-callback-method>
    </post-activate>
    <remove-method>
        <method-name>logout</method-name>
    </remove-method>
</session>