<?xml version="1.0" encoding="UTF-8"?>
<session xmlns="http://www.appserver.io/appserver">
    <session-type>Stateful</session-type>
    <epb-name>UserProcessor</epb-name>
    <epb-class>AppserverIo\Apps\Example\Services\UserProcessor</epb-class>
    <pre-destroy>
        <lifecycle-callback-method>destroy</lifecycle-callback-method>
    </pre-destroy>
    <pre-attach>
        <lifecycle-callback-method>attach</lifecycle-callback-method>
    </pre-attach>
    <post-detach>
        <lifecycle-callback-method>detach</lifecycle-callback-method>
    </post-detach>
    <pre-passivate>
        <lifecycle-callback-method>passivate</lifecycle-callback-method>
    </pre-passivate>
    <post-activate>
        <lifecycle-callback-method>activate</lifecycle-callback-method>
    </post-activate>
</session>